<div class="card p-4">
    {{ form_start(form) }}
        <div class="mb-3">
            {{ form_label(form.dateSoutenance) }}
            {{ form_widget(form.dateSoutenance, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="mb-3">
            {{ form_label(form.note) }}
            {{ form_widget(form.note, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="mb-3">
            {{ form_label(form.enseignant) }}
            {{ form_widget(form.enseignant, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="mb-3">
            {{ form_label(form.etudiant) }}
            {{ form_widget(form.etudiant, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="mt-4">
            <button type="submit" class="btn btn-primary">
                Add
            </button>
        </div>
    {{ form_end(form) }}
</div>
